import java.util.PriorityQueue;
import java.util.Comparator;

class Cliente {
    private int id;
    private String nome;
    private int prioridade;

    public Cliente(int id, String nome, int prioridade) {
        this.id = id;
        this.nome = nome;
        this.prioridade = prioridade;
    }

    public int getPrioridade() {
        return prioridade;
    }

    @Override
    public String toString() {
        return "Cliente{" +
                "id=" + id +
                ", nome='" + nome + '\'' +
                ", prioridade=" + prioridade +
                '}';
    }
}

class ComparadorDePrioridade implements Comparator<Cliente> {
    @Override
    public int compare(Cliente c1, Cliente c2) {
        return Integer.compare(c2.getPrioridade(), c1.getPrioridade());
    }
}

public class FilaLoterica {
    public static void main(String[] args) {
        PriorityQueue<Cliente> filaDePrioridade = new PriorityQueue<>(new ComparadorDePrioridade());

        filaDePrioridade.add(new Cliente(1, "Jo√£o", 2));
        filaDePrioridade.add(new Cliente(2, "Maria", 1));
        filaDePrioridade.add(new Cliente(3, "Ana", 3));

        while (!filaDePrioridade.isEmpty()) {
            System.out.println(filaDePrioridade.poll());
        }
    }
}
