import java.util.Comparator;
import java.util.PriorityQueue;

class Project {
    private int id;
    private String projectName;
    private int priority;

    public Project(int id, String projectName, int priority) {
        this.id = id;
        this.projectName = projectName;
        this.priority = priority;
    }

    public int getPriority() {
        return priority;
    }

    public String toString() {
        return "Projeto{" + "id = " + id + ", projectName = " + projectName + ", priority = " + priority + "}";
    }
}

class PriorityComparator implements Comparator<Project> {
    
    public int compare(Project p1, Project p2) {
        return Integer.compare(p2.getPriority(), p1.getPriority());
    }
}

public class ProjectDemand {
    public static void main(String[] args) {
        PriorityQueue<Project> priorityQueue = new PriorityQueue<>(new PriorityComparator());

        priorityQueue.offer(new Project(1, "Supermercado Mill", 1));
        priorityQueue.offer(new Project(2, "Nasa ltda", 3));
        priorityQueue.offer(new Project(3, "World Beckup", 10));
        priorityQueue.add(new Project(4, "Sistema Nacional do Brasil", 2));
        priorityQueue.add(new Project(5, "Sistema de Lanchonete", 1));

        while (!priorityQueue.isEmpty()) {
            // priorityQueue.poll() = priorityQueue.remove()
            System.out.println(priorityQueue.poll());
        }
    }
}
